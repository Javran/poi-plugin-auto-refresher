{
  "name": "poi-plugin-auto-refresher",
  "version": "0.2.0",
  "description": "Avoid unwelcome nodes or edges by refreshing. Use at your own risk.",
  "main": "index.es",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "lint": "eslint '**/*.es' || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Javran/poi-plugin-auto-refresher.git"
  },
  "keywords": [
    "kancolle",
    "poi"
  ],
  "author": {
    "name": "Javran",
    "url": "https://github.com/Javran"
  },
  "license": "MIT",
  "poiPlugin": {
    "title": "Auto Refresher",
    "priority": 20,
    "icon": "refresh"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.1.0",
    "fs-extra": "^5.0.0",
    "mocha": "^4.0.1",
    "path-extra": "^4.2.1"
  },
  "dependencies": {
    "parsimmon": "^1.2.0"
  }
